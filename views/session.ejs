<div class="row mt-5">
  <div class="col-md-12 m-auto">
    <div class="card card-body">
      <h1 class="text-center mb-3">Session</h1>
      <p>Log in at: <%= tag %></p>
      <div class="row mt-5">
        <div class="col-md-12">
          <h2 class="text-center mb-3">Question:</h2>
        </div>
      </div>
      <div class="row mt-5">
        <div class="col-md-10 m-auto">
          <% session.questions.forEach(function(question){ %>
          <div class="questions">
            <div class="row">
              <% if(question.image) { %> <img src=<%= question.image %>> <% } %>
              <h3 class="text-center mb-3"><%= question.text %></h3>
            </div>
          </div>
          <div class="row">
            <% question.answers.forEach(function(answer){ %>
            <div class="col-md-5 ml-3 mr-3">
              <p class="text-center"><%= answer %></p>
            </div>
            <% }); %>
          </div>

          <% }); %>
        </div>
      </div>
    </div>
  </div>
</div>
